<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.lit.original.PinActivity">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="9" >

            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/imageButton101"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@drawable/pin1"
                android:onClick="f9"
                android:adjustViewBounds="false" />

            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/imageButton102"
                android:layout_margin="1dp"
                android:layout_weight="1"
                android:background="@drawable/pin2"
                android:onClick="f10"
                android:nestedScrollingEnabled="false" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="9" >

            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_margin="1dp"
                android:background="@drawable/pin5"
                android:onClick="f11"
                android:nestedScrollingEnabled="false"
                android:id="@+id/imageButton103" />

            <ImageButton
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/imageButton104"
                android:layout_weight="1"
                android:layout_margin="1dp"
                android:background="@drawable/pin6"
                android:onClick="f12" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2" >

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="戻る"
                android:id="@+id/button11"
                android:textSize="25dp"
                android:onClick="f32"
                android:layout_weight="1"
                android:layout_margin="1dp" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="最初に戻る"
                android:id="@+id/button12"
                android:layout_weight="1"
                android:layout_margin="1dp"
                android:textSize="25dp"
                android:onClick="f30" />
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>
