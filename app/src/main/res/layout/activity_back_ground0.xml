<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".BackGround0Activity"
    android:background="#e5e4e4">

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1" >

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="食べ物"
                android:id="@+id/button21"
                android:layout_alignParentTop="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true"
                android:layout_weight="1"
                android:layout_margin="1dp"
                android:onClick="a01"
                android:textSize="25dp"
                android:background="@drawable/ba" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="風景"
                android:id="@+id/button22"
                android:layout_alignParentBottom="true"
                android:layout_alignLeft="@+id/button24"
                android:layout_alignStart="@+id/button24"
                android:layout_weight="1"
                android:layout_margin="1dp"
                android:textSize="25dp"
                android:onClick="g01"
                android:background="@drawable/bg" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1" >

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/button23"
                android:layout_below="@+id/button21"
                android:layout_alignRight="@+id/button21"
                android:layout_alignEnd="@+id/button21"
                android:layout_weight="1"
                android:layout_margin="1dp"
                android:textSize="25dp"
                android:text="空"
                android:onClick="b01"
                android:background="@drawable/bb" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="海"
                android:id="@+id/button24"
                android:layout_below="@+id/button23"
                android:layout_toLeftOf="@+id/button23"
                android:layout_toStartOf="@+id/button23"
                android:layout_weight="1"
                android:layout_margin="1dp"
                android:textSize="25dp"
                android:onClick="c01"
                android:background="@drawable/bc" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1" >

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="植物（部分）"
                android:id="@+id/button25"
                android:layout_alignTop="@+id/button24"
                android:layout_toRightOf="@+id/button24"
                android:layout_toEndOf="@+id/button24"
                android:layout_weight="1"
                android:layout_margin="1dp"
                android:textSize="25dp"
                android:onClick="d01"
                android:background="@drawable/bd" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="植物（全体）"
                android:id="@+id/button26"
                android:layout_centerVertical="true"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_weight="1"
                android:layout_margin="1dp"
                android:textSize="25dp"
                android:onClick="e01"
                android:background="@drawable/be" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1" >

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="模様"
                android:id="@+id/button27"
                android:layout_below="@+id/button29"
                android:layout_toRightOf="@+id/button26"
                android:layout_toEndOf="@+id/button26"
                android:layout_weight="1"
                android:layout_margin="1dp"
                android:textSize="25dp"
                android:onClick="h01"
                android:background="@drawable/bh" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="色"
                android:id="@+id/button28"
                android:layout_alignTop="@+id/button27"
                android:layout_alignLeft="@+id/button22"
                android:layout_alignStart="@+id/button22"
                android:layout_weight="1"
                android:layout_margin="1dp"
                android:textSize="25dp"
                android:onClick="i01"
                android:background="@drawable/bi" />
        </LinearLayout>

        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1" >

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="壁、床"
                android:id="@+id/button29"
                android:layout_alignTop="@+id/button26"
                android:layout_toRightOf="@+id/button24"
                android:layout_toEndOf="@+id/button24"
                android:layout_weight="1"
                android:layout_margin="1dp"
                android:textSize="25dp"
                android:onClick="f01"
                android:background="@drawable/bf" />

            <Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="その他"
                android:id="@+id/button30"
                android:layout_above="@+id/button29"
                android:layout_alignRight="@+id/button25"
                android:layout_alignEnd="@+id/button25"
                android:layout_weight="1"
                android:layout_margin="1dp"
                android:textSize="25dp"
                android:onClick="j01"
                android:background="@drawable/bj" />
        </LinearLayout>
    </LinearLayout>
</RelativeLayout>
